{
  "name": "@car2db/mcp-server",
  "version": "1.0.1",
  "description": "MCP server for Car2DB API v3, allowing LLM agents to work with the automotive database",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "car2db-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "car2db",
    "vehicle",
    "automotive",
    "api",
    "model-context-protocol"
  ],
  "author": "Car2DB",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/car2db/car2db-api-v3-mcp.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
